{
	"name": "vandarpay/exception-monitoring",
	"description": "Vandar Exception Monitoring Laravel Package",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": "^7.4|^8.0",
		"laravel/framework": "^7.0|^8.0|^9.0"
	},
	"authors": [
		{
			"name": "Vandar",
			"email": "info@vandar.io"
		},
		{
			"name": "Farshid Rezaei",
			"email": "farshid.net1@yahoo.com",
			"homepage": "https://farshidrezaei.ir",
			"role": "Full-Stack Web Developer"
		}
	],
	"extra": {
		"laravel": {
			"providers": [
				"VandarPay\\ExceptionMonitoring\\Providers\\ExceptionMonitoringServiceProvider"
			],
			"aliases": [
				"VandarPay\\ExceptionMonitoring\\Facades\\ExceptionMonitoring"
			]
		}
	},
	"autoload": {
		"files": [
			"src/Helpers/helpers.php"
		],
		"psr-4": {
			"VandarPay\\ExceptionMonitoring\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"optimize-autoloader": true
	}
}
